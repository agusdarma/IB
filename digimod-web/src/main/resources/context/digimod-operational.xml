			</result>
		</action>
		
		<action name="CreateIdBillingApproval!*" method="{1}" class="id.co.emobile.samba.web.action.operational.CreateIdBillingApprovalAction">
			<interceptor-ref name="webSecureStack" />
			<result name="input">WEB-INF/pages/operational/pbb/create_id_billing_approval_input.jsp</result>
			<result name="detail">WEB-INF/pages/operational/pbb/create_id_billing_approval_detail.jsp</result>
			<result name="detail2">WEB-INF/pages/operational/pbb/create_id_billing_approval_detail2.jsp</result>
			<result name="success" type="redirectAction">CreateIdBillingApproval!finish</result>
			<result name="finish">WEB-INF/pages/operational/pbb/create_id_billing_approval_finish.jsp</result>
			
			<result name="otp" type="json">
				<param name="root">wrv</param>
			</result>
			
			<result name="process" type="json">
				<param name="root">wrv</param>
			</result> 
		</action>
        
	</package>
</struts>
