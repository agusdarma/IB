			<result name="otp" type="json">
				<param name="root">wrv</param>
			</result>
		</action>
		
		<action name="CreateIdBillingCallback!*" method="{1}" class="id.co.emobile.samba.web.action.operational.CreateIdBillingCallbackAction">
			<interceptor-ref name="webSecureStack" />
			<result name="input">WEB-INF/pages/operational/pbb/create_id_billing_callback_input.jsp</result>
			<result name="detail">WEB-INF/pages/operational/pbb/create_id_billing_callback_detail.jsp</result>
			<result name="success" type="redirectAction">CreateIdBillingCallback!finish</result>
			<result name="finish">WEB-INF/pages/operational/pbb/create_id_billing_callback_finish.jsp</result>
			
